.mine{
  background: #F8F8F8;
}
.mine .user_infos{
  background-image: url('data:image/png;base64,');
  height: 442rpx;
  width: auto;
  background-size: 100% 100%;
  box-shadow: 0px 0px 7px #888;
  margin: 20rpx 20rpx;
  background-position: center center;
  background-repeat: no-repeat;
  border-radius: 26rpx;
}
.mine .user_infos .first{
  padding: 20rpx 0rpx 0rpx 45rpx;
  display:flex;
  justify-content: flex-start;
  align-items: center;
}
.mine .user_infos .first .infos .name{
  display: flex;
  align-items: center;

}
.mine .user_infos .first .infos .refresh{
  width: 28rpx !important;
  height: 28rpx !important;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background: none;
  margin: 0 10rpx;
}
.mine .user_infos .first .infos .common{
  width: 76.66666rpx;
  height: 28.33333rpx;
  margin: auto 0;
  margin-left: 12rpx;
}
.vips image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
  margin-top: 5rpx;
}
.mine .user_infos .first .avart image{
  width: 102rpx;
  height: 102rpx;
  border-radius: 50%;
  margin-right: 27rpx;
}
.mine .user_infos .first .signIn{
  color: #fff;
  font-weight: bold;
  font-size: 36rpx;
  padding: 0rpx 20rpx;
  background: rgba(255,255,255,0);
  width: auto;
}
.mine .user_infos .second{
  display:flex;
  justify-content: space-around;
  align-items: center;
  margin-bottom: 10rpx;
  margin-top: 35rpx;
}
.mine .user_infos .second .list_item{
  display:flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color:#fff;
  font-size: 26rpx;
}
.mine .user_infos .second .list_item text{
  padding: 5rpx 0;
}
.mine .user_infos .second .list_item .font{
  font-size: 44rpx;
}
.mine .user_infos .three{
  background: #fff;
  height:138rpx;
  border-radius: 0rpx 0rpx 24rpx 24rpx;
  display:flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.mine .user_infos .three image{
  width:533rpx;
  height:80rpx;
}
.mine .user_infos .three view{
  font-size:27rpx;
  font-weight:500;
  color:rgba(143,143,143,1);
}
.mine .first_item{
  height:233rpx;
}
.mine .orders{
  width: 100%;
  background:rgba(255,255,255,1);
  margin-top: 20rpx;
  font-size:32rpx;
  color:rgba(0,0,0,1);
  padding: 20rpx 20rpx;
  box-sizing: border-box;
}
.mine .order{
  display:flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 31rpx;
}
.mine .orders .order .my_order{
  text-indent: 12rpx;
  display:flex;
  align-items: center;
}
.mine .orders .order .my_order:before{
  content: "";
  display:inline-block;
  width: 6rpx;
  height: 30rpx;
  background-color: #50dad2;
}
.mine .order .all_order{
  display:flex;
  align-items: center;
}
.mine .order .all_order:after{
  content: '';
  width: 16rpx;
  height: 16rpx;
  border-top: 3rpx solid #999;
  border-right: 3rpx solid #999;
  transform: rotate(45deg);
  margin-right: 8rpx;
}
.mine .order_type {
  display:flex;
  justify-content: space-around;
  align-items: center;
  font-size: 24rpx;
}
.mine .order_type .order_item{
  display:flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.mine .order_type .order_item image{
  width:60rpx;
  height:57rpx;
  margin-bottom: 23rpx;
}
.mine .lists{
  background: #fff;
  margin: 20rpx 0rpx;
  padding: 20rpx 20rpx;
}
.mine .lists .list_item,.mine .lists .list_phone{
  display:flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 62rpx;
}
.mine .lists .list_phone{
  margin-bottom: 0rpx;
}
.mine .lists .list_phone .phone{
  display:flex;
  align-items: center;
}
.mine .lists .list_phone .phone:after{
  content: '';
  width: 16rpx;
  height: 16rpx;
  border-top: 3rpx solid #999;
  border-right: 3rpx solid #999;
  transform: rotate(45deg);
  margin: 0rpx 8rpx;
}
.mine .lists .list_item:after{
  content: '';
  width: 16rpx;
  height: 16rpx;
  border-top: 3rpx solid #999;
  border-right: 3rpx solid #999;
  transform: rotate(45deg);
  margin-right: 8rpx;
}
.mine .levels{
  display:flex;
  justify-content: flex-start;
  align-items: center;
  padding: 0 37rpx;
  font-size: 24rpx;
}
.mine .levels image{
  width:65rpx;
  height:90rpx;
  margin-right: 17rpx;
}
.mine .levels .progress{
  margin: 0 20rpx;
  position:relative;
}
.mine .levels .progress image{
  width: 30rpx;
  height: 44rpx;
  position: absolute;
  top: -28rpx;
}
.mine .levels .progress .line{
  width:367rpx;
  height:5rpx;
  background:linear-gradient(90deg,rgba(224,224,224,1),rgba(179,179,179,1));
}
.levels .card_type{
  color:#50DAD2;
}
.mine .levels .progress .desc{
  position: absolute;
  top: -65rpx;
  left: -73rpx;
}
.mine .vip_infos{
  display:flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 20rpx;
}
.mine .vip_infos .users{
  display:flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  flex: 6;
}
 .mine .vip_infos .users .user_avart{
  display:flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 54rpx;
   flex: 1;
}
.mine .vip_infos .users .user_avart image{
  width:100rpx;
  height:100rpx;
  border-radius:50%;
  margin-right: 20rpx;
}
.mine .vip_infos .counts{
  flex: 3;
  display:flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.mine .vip_infos .counts text{
  margin-bottom: 15rpx;
}
.mine .user_add{
  display:flex;
  justify-content: center;
  align-items: center;
  margin-top: 40rpx;
  font-size:28rpx;
  font-weight:500;
  color:rgba(143,143,143,1);
}
.mine .user_add image{
  width:48rpx;
  height:48rpx;
  border-radius:50%;
  margin-right: 18rpx;
}
